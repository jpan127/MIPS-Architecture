main:
    addi $4, $0, 4
    jal  factorial
    add $6, $7, $0
    nop                 # Wait for result
    j end

factorial:
    addi $sp, $sp, -8
    sw   $4,  4($sp)
    sw   $ra, 0($sp)
    addi $8, $0, 2
    slt  $8, $4, $8
    nop                 # Needs nops because forwarding + stalling does not work for branching in decode...
    nop
    beq  $8, $0, else
    addi $7, $0, 1
    addi $sp, $sp, 8
    jr   $ra
    nop

else:
    addi  $4, $4, -1
    jal factorial
    nop
    lw    $ra, 0($sp)
    lw    $4, 4($sp)
    addi  $sp, $sp, 8
    multu $4, $7
    mflo  $7
    jr    $ra
    nop

end:
    j end